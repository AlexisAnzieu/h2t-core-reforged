  "main": "index.js",
  "scripts": {
    "build": "npm nexus:reflect && tsc",
    "dev:migrate": "prisma migrate save --experimental -c && prisma migrate up --experimental -c",
    "dev:typecheck": "tsc --watch --noEmit",
    "nexus:reflect": "NEXUS_SHOULD_EXIT_AFTER_GENERATE_ARTIFACTS=true ts-node --transpile-only ./src/index",
    "start": "NODE_ENV=prod ts-node-dev --transpile-only ./src/index"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --fix --ext .ts src/ && npx sort-package-json"
    }
  },
  "dependencies": {
    "@nexus/schema": "^0.19.1",
    "@prisma/client": "^2.13.0",
