    "@types/nodemailer": "^6.4.0",
    "apollo-server": "^2.19.0",
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.23.0",
    "dotenv": "^8.2.0",
    "file-type": "^16.0.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "graphql-upload": "^11.0.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "nexus-plugin-prisma": "^0.26.0",
    "nodemailer": "^6.4.14",
    "npm": "^6.14.8",
    "path": "^0.12.7",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
